{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ccportal"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../public/client",
      "assets": [
        "assets",
        "favicon.ico",
        "robots.txt"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/sweetalert2/dist/sweetalert2.min.css",
        "./assets/plugins/metis-menu/metisMenu.min.css",
        "../node_modules/ngx-toastr/toastr.css",
        "./assets/css/style.css",
        "./assets/plugins/ui/jquery-ui.css",
        "./assets/css/theme/dark.css",
        "./assets/plugins/morris/morris.css",
        "./assets/plugins/simple-line-icons-master/css/simple-line-icons.css",
        "./assets/plugins/widget/widget.css",
        "./assets/plugins/tablesaw/tablesaw.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "./assets/plugins/ui/jquery-ui.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/sweetalert2/dist/sweetalert2.min.js",
        "../node_modules/sortablejs/Sortable.min.js",
        "./assets/plugins/metis-menu/metisMenu.min.js",
        "./assets/plugins/slim-scroll/jquery.slimscroll.min.js",
        "./assets/plugins/morris/raphael-2.1.0.min.js",
        "./assets/plugins/morris/morris.min.js",
        "./assets/js/custom.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
